# Configurações do Django
DJANGO_DEBUG=True
DJANGO_SECRET_KEY=your-secret-key-change-me
ALLOWED_HOSTS=localhost,127.0.0.1

# Configurações do Banco de Dados
DATABASE_URL=postgres://user:password@localhost:5432/dbname

# Chaves da API HERE
HERE_API_KEY_1=your-here-api-key-1
HERE_API_KEY_2=your-here-api-key-2
HERE_API_KEY_3=your-here-api-key-3

# Google Gemini API Key
GEMINI_API_KEY=your-gemini-api-key

# Google Maps API Key (Exemplo - o nome pode ser ligeiramente diferente)
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY 

# Configurações de Ambiente
ENVIRONMENT=development

# Google OAuth - Desenvolvimento
GOOGLE_CLIENT_ID_DEV=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET_DEV=your-google-client-secret

# Google OAuth - Produção
GOOGLE_CLIENT_ID_PROD=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET_PROD=your-google-client-secret

# Cloudinary
CLOUDINARY_CLOUD_NAME=your-cloudinary-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret 